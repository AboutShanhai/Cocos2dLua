                  <AbstractNodeData Name="Title" ActionTag="-177014618" Tag="106" IconVisible="False" HorizontalEdge="LeftEdge" VerticalEdge="TopEdge" RightMargin="1159.0000" BottomMargin="3064.0000" FontSize="30" LabelText="游戏规则" OutlineSize="0" ShadowOffsetX="2.0000" ShadowOffsetY="-2.0000" ctype="TextObjectData">
